{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/server.js"
    }
  ],
  "functions": {
    "server.js": {
      "includeFiles": "{model_data.json,dict/**}"
    }
  },
  "crons": [
    {
      "path": "/api/batch/calculate-ranking",
      "schedule": "0 15 * * *"
    }
  ]
}